<?xml version="1.0" encoding="utf-8"?>
<!-- Struts2 DTD -->
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
	<constant name="struts.i18n.encoding" value="utf-8" />
	<constant name="struts.devMode" value="true" />
	<package name="process" extends="struts-default">
		<!-- Example
		<action name="userRegister" class="action.userRegisterAction">
			<result name="success">/index.jsp</result>
			<result name="error">/username_exist.jsp</result>
			<result name="input">/password_error.jsp</result>
		</action>
		-->
		<action name="Register" class="registerAction">
			<result name="success">/index.jsp</result>
			<result name="error">/username_exist.jsp</result>
			<result name="input">/repsw_error.jsp</result>
		</action>
		<action name="Login" class="koginAction">
			<result name="success">/index.jsp</result>
			<result name="input">/psw_error.jsp</result>
		</action>
		<action name="getData" class="getDataAction">
			<result name="success">/firstPage.jsp</result>
		</action>
		<action name="modifyProfile" class="modifyProfileAction">
			<result name="success">/profile.jsp</result>
		</action>
		
	</package>
</struts>