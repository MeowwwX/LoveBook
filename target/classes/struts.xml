<?xml version="1.0" encoding="utf-8"?>
<!-- Struts2 DTD -->
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
	<constant name="struts.i18n.encoding" value="utf-8" />
	<constant name="struts.devMode" value="true" />
	<package name="process" extends="struts-default">
		<action name="userRegister" class="action.registerAction">
			<result name="success">/index.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="userLogin" class="action.loginAction">
			<result name="success">/index.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="quitLogin" class="action.quitLoginAction">
			<result name="success">/index.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="index" class="action.getDataAction">
			<result name="success">/firstPage.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="search" class="action.getDataAction">
			<result name="success">/firstPage.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="modifyProfile" class="action.modifyProfileAction">
			<result name="success">/profile.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="publishBook" class="action.publishBookAction">
			<result name="success">/index.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="borrowBook" class="action.borrowBookAction">
			<result name="success">/index.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="returnBook" class="action.returnBookAction">
			<result name="success">/index.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="publish" class="action.getSpecificUserAction">
			<result name="success">/publish.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="getSpecificBook" class="action.getSpecificBookAction">
			<result name="success">/specificBook.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="manageSpecificBook" class="action.getSpecificBookAction">
			<result name="success">/modBook.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="profile" class="action.getSpecificUserAction">
			<result name="success">/profile.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="getPublished" class="action.getBooksByOwnerAction">
			<result name="success">/published.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="getBorrowed" class="action.getBorrowedAction">
			<result name="success">/borrowed.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="postComment" class="action.postCommentAction">
			<result name="success">/index.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="deleteComment" class="action.deleteCommentAction">
			<result name="success">/index.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="getManage" class="action.getManageDataAction">
			<result name="success">/manageUser.jsp</result>
			<result name="input">/manageBook.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="manageBook" class="action.getDataAction">
			<result name="success">/manageBook.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="manageUser" class="action.getDataAction">
			<result name="success">/manageUser.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="delBook" class="action.delBookAction">
			<result name="success">/manage.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="modBook" class="action.modBookAction">
			<result name="success">/manage.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="delUser" class="action.delUserAction">
			<result name="success">/manage.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="modUser" class="action.modUserAction">
			<result name="success">/manage.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
	</package>
</struts>